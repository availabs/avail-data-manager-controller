{
  "name": "avail-datamanager-controller",
  "version": "0.0.2",
  "description": "AVAIL Data Manager Controller",
  "engines": {
    "node": ">= 16.x.x <17.x.x"
  },
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect --require ts-node/register ./node_modules/moleculer/bin/moleculer-runner.js --env --hot --repl --config moleculer.config.ts 'src/\\*\\*/\\*.service.ts'",
    "dev": "NODE_OPTIONS='--max-old-space-size=10000' ts-node -T ./node_modules/moleculer/bin/moleculer-runner.js --env --hot --repl --config moleculer.config.ts src/\\*\\*/*.service.ts",
    "start": "moleculer-runner --config dist/moleculer.config.js",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "@types/compression": "^1.7.2",
    "@types/decompress": "^4.2.4",
    "@types/dedent": "^0.7.0",
    "@types/express-http-proxy": "^1.6.3",
    "@types/geojson": "^7946.0.10",
    "@types/graphlib": "^2.1.8",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.185",
    "@types/luxon": "^3.2.0",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^18.7.18",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/pg-cursor": "^2.7.0",
    "@types/pg-format": "^1.0.2",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.7.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "axios": "^1.1.3",
    "better-sqlite3": "^7.6.2",
    "busboy": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "decompress": "^4.2.1",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.2",
    "express-http-proxy": "^1.6.3",
    "fast-csv": "^4.3.6",
    "flux-standard-action": "^2.1.2",
    "form-data": "^4.0.0",
    "gdal-async": "3.5.1",
    "graphlib": "^2.1.8",
    "gunzip-maybe": "^1.4.2",
    "jest": "^29.3.1",
    "json-stable-stringify": "^1.0.2",
    "luxon": "^3.2.1",
    "memoize-one": "^6.0.0",
    "moleculer": "^0.14.23",
    "moleculer-db": "^0.8.19",
    "moleculer-db-adapter-mongo": "^0.4.15",
    "moleculer-web": "^0.10.4",
    "nats": "^1.3.2",
    "ndjson": "^2.0.0",
    "ndjson-cli": "^0.3.1",
    "node-fetch": "^2.6.7",
    "p-event": "^4.2.0",
    "pg": "^8.8.0",
    "pg-copy-streams": "^6.0.4",
    "pg-cursor": "^2.7.4",
    "pg-format": "^1.0.4",
    "pg-query-stream": "^4.2.4",
    "save": "^2.9.0",
    "serve-static": "^1.15.0",
    "shapefile-to-geojson": "^1.0.1",
    "sql": "^0.78.0",
    "tileserver-gl-light": "^4.1.2",
    "tmp": "^0.2.1",
    "unzipper": "^0.10.11",
    "uuid": "^9.0.0",
    "wellknown": "^0.5.0"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
