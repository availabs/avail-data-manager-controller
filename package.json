{
  "name": "avail-datamanager-controller",
  "version": "0.0.2",
  "description": "AVAIL Data Manager Controller",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "debug": "node --inspect --require ts-node/register ./node_modules/moleculer/bin/moleculer-runner.js --env --hot --repl --config moleculer.config.ts 'src/\\*\\*/\\*.service.ts'",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' ts-node -T ./node_modules/moleculer/bin/moleculer-runner.js --env --hot --repl --config moleculer.config.ts src/\\*\\*/*.service.ts",
    "start": "moleculer-runner --config dist/moleculer.config.js",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.185",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^18.7.18",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.7.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "better-sqlite3": "^7.6.2",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "cross-fetch": "^3.1.5",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.2",
    "express-http-proxy": "^1.6.3",
    "flux-standard-action": "^2.1.2",
    "form-data": "^4.0.0",
    "moleculer": "^0.14.23",
    "moleculer-db": "^0.8.19",
    "moleculer-db-adapter-mongo": "^0.4.15",
    "moleculer-web": "^0.10.4",
    "nats": "^1.3.2",
    "ndjson": "^2.0.0",
    "ndjson-cli": "^0.3.1",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "shapefile-to-geojson": "^1.0.1",
    "shp2json": "^1.4.0",
    "sql": "^0.78.0",
    "topojson-server": "^3.0.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 14.x.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
